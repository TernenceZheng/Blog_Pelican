        &lt;span class="kn"&gt;from&lt;/span&gt; &lt;span class="nn"&gt;certbot&lt;/span&gt; &lt;span class="kn"&gt;import&lt;/span&gt; &lt;span class="n"&gt;constants&lt;/span&gt; &lt;span class="k"&gt;as&lt;/span&gt; &lt;span class="n"&gt;core_constants&lt;/span&gt;
